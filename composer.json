{
    "name" : "jrfnl/debug-bar-constants",
    "description" : "Debug Bar Constants adds new panels to the WordPress Debug Bar that display all the defined constants for the current request.",
    "keywords"   : ["wordpress", "plugin" ],
    "type"       : "wordpress-plugin",
    "license"    : "GPL-2.0-or-later",
    "authors"    : [
        {
            "name"    : "Juliette Reinders Folmer",
            "homepage": "https://github.com/jrfnl"
        }
    ],
    "support"    : {
        "issues": "https://github.com/jrfnl/Debug-Bar-Constants/issues",
        "source": "https://github.com/jrfnl/Debug-Bar-Constants"
    },
    "require": {
        "php" : ">=5.2"
    },
    "require-dev": {
        "wp-coding-standards/wpcs": "^2.0",
        "phpcompatibility/phpcompatibility-wp": "^2.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.6.2 || ^0.7"
    },
    "minimum-stability" : "RC",
    "prefer-stable": true
}
